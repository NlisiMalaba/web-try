{
    "name": "healthassistpro/medication-reminders",
    "description": "SMS Medication Reminders for HealthAssist Pro",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "twilio/sdk": "^7.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "HealthAssistPro\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
